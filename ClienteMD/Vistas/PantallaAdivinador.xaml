        Background="Transparent"
        AllowsTransparency="True"
        ResizeMode="NoResize"
        Height="650" Width="900"
        WindowStartupLocation="CenterScreen">
    <Border Background="#1B2430" CornerRadius="20" MouseDown="moverVentana">
